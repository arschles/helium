{
    "name": "helium-runner",
    "version": "0.1.0",
    "description": "The Javascript runner for Helium builds",
    "main": "dist/index",
    "types": "dist/index",
    "scripts": {
        "initialize": "./prepare.sh",
        "start": "node --no-deprecation ./dist/src/index.js",
        "test": "tsc && mocha ./dist/test",
        "build": "tsc",
        "helium": "tsc && yarn start",
        "docs": "typedoc --out ./doc --excludePrivate --excludeExternals --exclude test/*.ts --exclude doc/** ./src"
    },
    "author": "Aaron Schlesinger",
    "license": "Apache-2.0",
    "repository": "https://github.com/kubehelium/helium",
    "devDependencies": {
        "@types/chai": "^4.0.1",
        "@types/mocha": "^2.2.41",
        "@types/node": "^8.0.14",
        "chai": "^4.1.0",
        "mocha": "^3.4.2",
        "typedoc": "^0.8.0",
        "typescript": "^2.4.2"
    },
    "dependencies": {
        "concat": "^1.0.3",
        "copy": "^0.3.1",
        "immutable": "^3.8.2",
        "ulid": "^0.2.0"
    }
}
